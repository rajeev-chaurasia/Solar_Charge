#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/solc@0.4.26/node_modules/solc/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/solc@0.4.26/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules:/home/maverick/Desktop/Solar-Charge/node_modules:/home/maverick/Desktop/node_modules:/home/maverick/node_modules:/home/node_modules:/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules/@ensdomains/ens/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules/@ensdomains/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/solc@0.4.26/node_modules/solc/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/solc@0.4.26/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules:/home/maverick/Desktop/Solar-Charge/node_modules:/home/maverick/Desktop/node_modules:/home/maverick/node_modules:/home/node_modules:/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules/@ensdomains/ens/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules/@ensdomains/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/@ensdomains+ens@0.4.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../solc@0.4.26/node_modules/solc/solcjs" "$@"
else
  exec node  "$basedir/../../../../../../solc@0.4.26/node_modules/solc/solcjs" "$@"
fi
