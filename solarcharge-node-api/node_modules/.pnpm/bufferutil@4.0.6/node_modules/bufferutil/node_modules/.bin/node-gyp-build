#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node-gyp-build@4.4.0/node_modules/node-gyp-build/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node-gyp-build@4.4.0/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules:/home/maverick/Desktop/Solar-Charge/node_modules:/home/maverick/Desktop/node_modules:/home/maverick/node_modules:/home/node_modules:/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/bufferutil@4.0.6/node_modules/bufferutil/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/bufferutil@4.0.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node-gyp-build@4.4.0/node_modules/node-gyp-build/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node-gyp-build@4.4.0/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules:/home/maverick/Desktop/Solar-Charge/node_modules:/home/maverick/Desktop/node_modules:/home/maverick/node_modules:/home/node_modules:/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/bufferutil@4.0.6/node_modules/bufferutil/node_modules:/home/maverick/Desktop/Solar-Charge/solarcharge-node-api/node_modules/.pnpm/bufferutil@4.0.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp-build@4.4.0/node_modules/node-gyp-build/bin.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp-build@4.4.0/node_modules/node-gyp-build/bin.js" "$@"
fi
